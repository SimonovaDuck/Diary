<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:app="http://schemas.android.com/apk/res-auto">




	<ScrollView android:id="@+id/emotions" 
		android:layout_width="wrap_content" 
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container" 
			android:layout_width="match_parent"
			android:layout_height="wrap_content" >

			<View   android:id="@+id/_bg__emotions"
				android:background="@drawable/_bg__emotions_shape"
				 android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:layout_width="match_parent"
				android:layout_height="800dp" />




			<View   android:id="@+id/rectangle_26"
				android:background="@drawable/rectangle_26_shape"
				 android:layout_marginStart="0dp"
				android:layout_marginTop="512dp"
				android:layout_width="match_parent"
				android:layout_height="96dp" />





			<TextView
				android:id="@+id/_5________"
				android:textStyle="italic"
				android:textSize="24sp"
				android:textColor="@color/_______ek2_color"
				android:layout_width="wrap_content"
			android:layout_height="42.4dp"
			android:layout_centerHorizontal="true"
			android:layout_marginTop="440dp"
			android:text="число" />

			<TextView android:id="@+id/_________________________"
				android:textStyle="italic"
				android:textSize="18sp"
				android:textColor="@color/_______ek2_color"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="489dp"
				android:layout_width="258.8dp"
				android:layout_height="26.8dp"
				android:text="Ваши чувства в этот день:"
				 />

		</RelativeLayout>

	</ScrollView>


	<RelativeLayout
		android:id="@+id/head"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="0dp"
		android:layout_marginStart="0dp">

		<View
			android:id="@+id/rectangle_1"
			android:layout_width="match_parent"
			android:layout_height="78dp"
			android:layout_marginStart="0dp"
			android:layout_marginTop="0dp"
			android:background="@drawable/rectangle_1_shape" />

		<TextView
			android:id="@+id/diary"
			android:layout_width="89dp"
			android:layout_height="53dp"
			android:layout_gravity="center"
			android:layout_marginLeft="147dp"
			android:layout_marginTop="19dp"
			android:fontFamily="@font/kav"
			android:gravity="center"
			android:text="@string/diary_string"
			android:textColor="@color/______________color"
			android:textSize="30sp"
			android:layout_centerVertical="true" />

		<ImageView
			android:id="@+id/create_new_text"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:layout_alignParentStart="true"
			android:layout_marginStart="45dp"
			android:layout_centerVertical="true"
			android:src="@drawable/new_text" />

		<ImageView
			android:id="@+id/stat"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:layout_alignParentEnd="true"
			android:layout_marginEnd="45dp"
			android:layout_centerVertical="true"
			android:src="@drawable/statistic" />

	</RelativeLayout>


	<LinearLayout
		android:id="@+id/footer111"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:orientation="horizontal"
		android:layout_marginTop="78dp"
		app:layout_constraintTop_toTopOf="parent">

		<RelativeLayout
			android:id="@+id/first_item"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1">

			<View
				android:id="@+id/_bg_________ek5"
				android:layout_width="match_parent"
				android:layout_height="54dp"
				android:background="@drawable/_bg_________ek5_shape" />

			<View
				android:id="@+id/rectangle_5"
				android:layout_width="match_parent"
				android:layout_height="54dp"
				android:background="@color/bled_purp" />

			<TextView
				android:id="@+id/notes"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_centerVertical="true"
				android:text="Записи"
				android:textColor="@color/_04_03_24_color"
				android:textSize="14sp"
				android:textStyle="italic" />

		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/second_item"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1">

			<View
				android:id="@+id/_bg___________ek1"
				android:layout_width="match_parent"
				android:layout_height="54dp"
				android:background="@drawable/_bg___________ek1_shape" />

			<View
				android:id="@+id/rectangle_6"
				android:layout_width="match_parent"
				android:layout_height="54dp"
				android:background="@drawable/rectangle_6_shape" />

			<TextView
				android:id="@+id/ex"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_centerVertical="true"
				android:text="Задания"
				android:textColor="@color/_04_03_24_color"
				android:textSize="14sp"
				android:textStyle="italic" />

		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/third_item"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1">

			<View
				android:id="@+id/rectangle_7"
				android:layout_width="match_parent"
				android:layout_height="54dp"
				android:background="@drawable/rectangle_7_shape" />

			<TextView
				android:id="@+id/wonts"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_centerVertical="true"
				android:text="Привычки"
				android:textColor="@color/_04_03_24_color"
				android:textSize="14sp"
				android:textStyle="italic" />

		</RelativeLayout>
	</LinearLayout>

	<View   android:id="@+id/foot"
		android:background="@drawable/foot_shape"
		android:layout_width="match_parent"
		android:layout_height="65dp"
		app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>